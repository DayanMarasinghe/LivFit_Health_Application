<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF"
    tools:context=".SearchWorkout">

    <SearchView
        android:id="@+id/searchViewworkout"
        android:layout_width="359dp"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnsrchwrkout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:backgroundTint="@color/blue"
        android:text="@string/searchw"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchViewworkout"
        app:strokeColor="#FFFFFF" />

    <ImageButton
        android:id="@+id/imageButtonarms"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButtonchest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageButtonlegs"
        app:layout_constraintTop_toBottomOf="@+id/btnsrchwrkout"
        app:srcCompat="@drawable/arms" />

    <ImageButton
        android:id="@+id/imageButtonlegs"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButtonback"
        app:layout_constraintEnd_toStartOf="@+id/imageButtonarms"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnsrchwrkout"
        app:srcCompat="@drawable/legs" />

    <ImageButton
        android:id="@+id/imageButtonback"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButtoncardio"
        app:layout_constraintEnd_toStartOf="@+id/imageButtonchest"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back" />

    <ImageButton
        android:id="@+id/imageButtonchest"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButtonshuol"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageButtonback"
        app:srcCompat="@drawable/chest" />

    <ImageButton
        android:id="@+id/imageButtoncardio"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButtonshuol"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cardio" />

    <ImageButton
        android:id="@+id/imageButtonshuol"
        android:layout_width="161dp"
        android:layout_height="156dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageButtoncardio"
        app:srcCompat="@drawable/shoulders" />

</androidx.constraintlayout.widget.ConstraintLayout>